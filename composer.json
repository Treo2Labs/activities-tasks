    "description": "ActivitiesTasks module for TreoPIM",
    "require": {
        "treolabs/treocore": ">=3.23.0 <3.26.0"
    },
    "autoload": {
        "psr-4": {
            "ActivitiesTasks\\": "app/"
        }
    },
    "extra": {
        "treoId": "ActivitiesTasks",
        "name": {
            "default": "Activities and Tasks"
